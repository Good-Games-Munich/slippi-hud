{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"default": ""
		},
		"round": {
			"type": "string",
			"default": ""
		},
		"bestOf": {
			"type": "number",
			"default": 1
		},
		"commentators": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "number",
						"default": 0
					},
					"name": {
						"type": "string",
						"default": ""
					},
					"info": {
						"type": "string",
						"default": ""
					}
				}
			}
		},
		"scores": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"score": {
						"type": "number",
						"default": 0
					},
					"rawResults": {
						"type": "array",
						"items": {
							"type": "number",
							"default": 0
						}
					}
				}
			}
		},
		"autoScore": {
			"type": "boolean",
			"default": false
		},
		"inputDisplay": {
			"type": "boolean",
			"default": false
		},
		"matchScored": {
			"type": "boolean",
			"default": false
		}
	}
}
